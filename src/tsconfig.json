{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": false,
        "moduleResolution": "node",
        "outDir": "../dist"
    },
    "include": [
        "./**/*"
    ],
    "compileOnSave": true
}
